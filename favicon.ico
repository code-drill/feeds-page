  <circle cx="22" cy="8" r="1" fill="#ffffff" opacity="0.7"/>
  <circle cx="25" cy="10" r="0.8" fill="#ffffff" opacity="0.5"/>
  <circle cx="20" cy="6" r="0.8" fill="#ffffff" opacity="0.5"/>
  <circle cx="27" cy="12" r="1" fill="#ffffff" opacity="0.6"/>
  
